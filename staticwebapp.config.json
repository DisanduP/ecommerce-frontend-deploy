{
  "name": "ecommerce-frontend",
  "buildCommand": "npm run build",
  "apiLocation": "",
  "appBuildCommand": "npm run build",
  "appLocation": "/",
  "platform": {
    "nodeVersion": "20"
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
